{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static-build",
        "config": {
          "rewrites": [
            {
              "source": "/index.html",
              "destination": "/[randomParameter].html"
            }
          ]
        }
      }
    ]
  }
  